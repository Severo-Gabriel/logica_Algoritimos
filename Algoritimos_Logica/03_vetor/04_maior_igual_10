algoritmo "maior_igual_10"
var
   A: vetor[1..15] de inteiro
   i: inteiro

inicio
   para i de 1 ate 15 faca
      escreva("Digite o valor de A[", i, "]: ")
      leia(A[i])
   fimpara

   escreva("Resultado dos numeros maiores ou igual a 10: ")
   para i de 1 ate 15 faca
      se (A[i] >= 10) entao
         escreva(A[i], " ")
      fimse
   fimpara
fimalgoritmo
